{"id":6,"name":"EV006","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":4,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":true,"image":{"tileId":512,"characterName":"","characterIndex":0,"direction":2,"pattern":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["宇宙射线被地球大气层影响，对地面单个人的天然本底辐射"]},{"code":401,"indent":0,"parameters":["仅为0.3-0.4 mSv/y。在大气层外，"]},{"code":401,"indent":0,"parameters":["每秒约有一个质子或更重的原子核穿过指甲大小的面积，"]},{"code":401,"indent":0,"parameters":["总共每秒约有5000个离子贯穿宇航员的身体,打断体内的化学键,"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["引起一连串电离反应。在宇宙射线中，"]},{"code":401,"indent":0,"parameters":["少数较重的核子会造成比质子更大的伤害，"]},{"code":401,"indent":0,"parameters":["因为打断化学键的能力与电荷平方成正比。"]},{"code":401,"indent":0,"parameters":["例如，铁原子核所造成的伤害是质子的676倍。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["根据美国航空航天局（NASA）的估计，"]},{"code":401,"indent":0,"parameters":["航天员在太阳系内的太空中每年受到250 mSv的辐射，"]},{"code":401,"indent":0,"parameters":["体内约有1/3的DNA会被宇宙射线切断。在月面是70-120mSv/y，"]},{"code":401,"indent":0,"parameters":["近地轨道是100mSv/y，范艾伦辐射带为15 Sv/y。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["太阳也会释放大量质子与重原子核，以接近光速喷出，"]},{"code":401,"indent":0,"parameters":["有时一小时内会逾数Sv，对没有屏障的航天员是致死剂量。"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":7,"y":3},
{"id":7,"name":"EV007","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":2,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":true,"image":{"tileId":513,"characterName":"","characterIndex":0,"direction":2,"pattern":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["冥想"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["自我，即人，由于受到自然界三种状态的蒙蔽，"]},{"code":401,"indent":0,"parameters":["所以是受控制、受支配的。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["如果一个人处于愚昧无知状态的影响支配下，"]},{"code":401,"indent":0,"parameters":["就会希望获得某一特定类型的感官享受，"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["他可能想睡眠或被麻醉；"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["如果一个人处于激情状态的影响支配下，"]},{"code":401,"indent":0,"parameters":["他可能要作性的享乐；"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["如果一个人是处于善良状态的影响支配下，"]},{"code":401,"indent":0,"parameters":["他可能要享受在公园或农村度过和平宁静的一天。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["因此，所有这些情况，是人的欲念的性质，"]},{"code":401,"indent":0,"parameters":["由自然界中哪一种影响支配着他的状态来决定的。"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":10,"y":3},
